    "def square():\n",
    "    a = 1\n",
    "    while True:\n",
    "        yield a*a\n",
    "        a+=1\n",
    "\n",
    "for i in square():\n",
    "    if(i >= 1000):\n",
    "        break\n",
    "    print(i, end=' ')"
