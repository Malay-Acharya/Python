    "class Employee:\n",
    "    def __init__(self, id, name):\n",
    "        self.id = id\n",
    "        self.name = name\n",
    "\n",
    "    def display(self):\n",
    "        print(f\"{self.id} {self.name}\")\n",
    "\n",
    "emp = Employee(1, \"coder\")\n",
    "emp.display()\n",
    "del emp.id\n",
    "del emp\n",
    "xy = Employee(2, \"okay\")\n",
    "xy.display()"
   ]
